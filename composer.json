{
  "name": "drupal/ace_editor",
  "type": "drupal-module",
  "description": "Ace is an embeddable code editor providing syntax highlighting and much more. See https://ace.c9.io/.",
  "keywords": [
    "Drupal",
    "ACE",
    "Editor"
  ],
  "license": "GPL-2.0+",
  "homepage": "https://www.drupal.org/project/ace_editor",
  "support": {
    "issues": "https://www.drupal.org/project/issues/ace_editor",
    "source": "http://cgit.drupalcode.org/ace_editor"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "ajaxorg/ace-builds",
        "version": "1.4",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/ajaxorg/ace-builds/archive/v1.4.0.zip",
          "type": "zip"
        }
      }
    }
  ],
  "require": {
    "ajaxorg/ace-builds": "~1.4.0"
  }
}
